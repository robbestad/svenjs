{
  "name": "svenjs",
  "version": "0.3.2",
  "description": "Micro javascript framework for composable web apps",
  "scripts": {
    "test": "tape tests/*",
    "_prebuild": "babel-cli src -d es5",
    "build": "NODE_ENV=production webpack",
    "postbuild": "cp assets/* npmbuild",
    "watch": "sane 'npm run build' --glob='src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svenanders/svenjs.git"
  },
  "main": "src/index.js",
  "directories": {
    "example": "example"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-loader": "^8.0.0-beta.4",
    "clone": "^2.1.2",
    "eslint": "^5.4.0",
    "glob": "~7.1.2",
    "jsdom": "^12.0.0",
    "jstransform": "^11.0.3",
    "object-assign": "^4.0.1",
    "rimraf": "^2.3.4",
    "root-require": "^0.3.1",
    "tape": "~4.9.1",
    "uglify-js": "^3.4.7",
    "webpack-cli": "^3.1.0",
    "webpack": "^4.17.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-serve": "^2.0.2"
  },
